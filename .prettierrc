{
	"printWidth": 150,
	"trailingComma": "none",
	"tabWidth": 4,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"bracketSpacing": true,
	"jsxBracketSameLine": true,
	"arrowParens": "avoid",
	"overrides": [{
			"files": ["*.json", ".eslintrc", ".tslintrc", ".prettierrc", ".tern-project"],
			"options": {
				"parser": "json",
				"tabWidth": 4
			}
		},
		{
			"files": "*.ts",
			"options": {
				"parser": "typescript"
			}
		}
	]
}